{
  "manifest_version": 2,
  "name": "Page Clipper",
  "description": "Clip parts of a web page.",
  "version": "1.0",
  "background": {
    "scripts": ["eventPage.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs",
    "contextMenus",
    "storage"
  ]
}